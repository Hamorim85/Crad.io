<p id="notice"><%= notice %></p>

<h1>Influencers</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Country</th>
      <th>Followers count</th>
      <th>Following count</th>
      <th>Bio</th>
      <th>Media count</th>
      <th>Igid</th>
      <th>Photo</th>
      <th>Full name</th>
      <th>Verified</th>
      <th>External url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @influencers.each do |influencer| %>
      <tr>
        <td><%= influencer.username %></td>
        <td><%= influencer.email %></td>
        <td><%= influencer.country %></td>
        <td><%= influencer.followers_count %></td>
        <td><%= influencer.following_count %></td>
        <td><%= influencer.bio %></td>
        <td><%= influencer.media_count %></td>
        <td><%= influencer.igid %></td>
        <td><%= influencer.photo %></td>
        <td><%= influencer.full_name %></td>
        <td><%= influencer.verified %></td>
        <td><%= influencer.external_url %></td>
        <td><%= link_to 'Show', influencer %></td>
        <td><%= link_to 'Edit', edit_influencer_path(influencer) %></td>
        <td><%= link_to 'Destroy', influencer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Influencer', new_influencer_path %>
